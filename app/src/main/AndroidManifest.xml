<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.adouglas.quizmaker">

    <application android:name="com.orm.SugarApp">
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:debuggable="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="Quizmaker">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".TestsActivity"
            android:label="Tests" />
        <!--
 ATTENTION: This was auto-generated to add Google Play services to your project for
     App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information.
        -->
        <meta-data
            android:name="DATABASE"
            android:value="quizmaker.db" />
        <meta-data
            android:name="VERSION"
            android:value="2" />
        <meta-data
            android:name="QUERY_LOG"
            android:value="true" />
        <meta-data
            android:name="DOMAIN_PACKAGE_NAME"
            android:value="com.adouglas.quizmaker" />
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
        android:debu
        <activity
            android:name=".TestIntroActivity"
            android:label="Test Introduction" />
        <activity
            android:name=".TestRunnerActivity"
            android:label="Test Running" />
        <activity
            android:name=".TestResultActivity"
            android:label="Test Result" />
        <activity
            android:name=".EditTestActivity"
            android:label="Edit Test" />
        <activity
            android:name=".QuestionsActivity"
            android:label="Questions" />
        <activity
            android:name=".AddQuestionActivity"
            android:label="Add Question" />
        <activity
            android:name=".ChoicesActivity"
            android:label="Multiple Choices" />
        <activity
            android:name=".AddChoiceActivity"
            android:label="Add Answer" />
        <activity
            android:name=".TestResultsActivity"
            android:label="Test Results"></activity>
        <activity android:name=".BaseActivity"></activity>
    </application>

</manifest>